#include <stdio.h>
#include <math.h>

// Function to check if a number is prime
int isPrime(int num) {
    if (num <= 1) {
        return 0; // 0 and 1 are not prime
    }
    for (int i = 2; i <= sqrt(num); i++) {
        if (num % i == 0) {
            return 0; // If the number is divisible by any number other than 1 and itself, it's not prime
        }
    }
    return 1; // If the number is not divisible by any number other than 1 and itself, it's prime
}

// Function to print prime numbers between two given numbers
void printPrimes(int start, int end) {
    printf("Prime numbers between %d and %d are:\n", start, end);
    for (int i = start; i <= end; i++) {
        if (isPrime(i)) {
            printf("%d\n", i);
        }
    }
}

int main() {
    int start, end;

    // Input two numbers
    printf("Enter the starting number: ");
    fflush(stdin);fflush(stdout);
    scanf("%d", &start);
    printf("Enter the ending number: ");
    fflush(stdin);fflush(stdout);
    scanf("%d", &end);

    // Print prime numbers between the given range
    printPrimes(start, end);

    return 0;
}
