#include "Stdio.h"
float ahmed(float t,float x)
{
	float m;
	m=t*x;
	return m;
}
void main()
{
	float x,y;
	printf("enter first number:\n");
	fflush(stdout);
	scanf("%f",&x);
	printf("enter second number:\n");
	fflush(stdout);
	scanf("%f",&y);
	float v=ahmed(x,y)+2;
	printf("%.2f",v);

}


