#include "stdio.h"

int square_root(int l)
{
float y;
y=sqrt(l);
	return y ;

}
int main()
{
	int n;
	printf("enter the number:");
	fflush(stdin);fflush(stdout);
	scanf("%d",&n);
	printf("the square root is:%.3f",square_root(n));

}

