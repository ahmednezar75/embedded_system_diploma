#include <stdio.h>
int prime(int i,int j);
void print(int i)
{
if (prime==1)
{
	printf("%d",i);
	}
}
int prime(int i,int j)
{
	for(;i<=j;i++)
	{
	for (int z=2;z<j;z++)
	{
		if(i%z ==0 && i!=z)
		{
			return 1;
		}

	}
	print(i);
	}
}

void main() {
	int x,y;
	printf("Enter the first number: \n");
	fflush(stdout);fflush(stdin);
	scanf("%d",&x);
	printf("Enter the second number: \n");
	fflush(stdout);fflush(stdin);
	scanf("%d",&y);
	prime(x,y);

}
