#include "stdio.h"

int sum(int n)
{
	if (n>0)
	{

		return n+sum(n-1);
	}
	else
	{
		return 0;
	}
}
int main()
{
	int len;
	int arr[100];
	printf("enter the length of the array:");
	fflush(stdin);fflush(stdout);
	scanf("%d",&len);
	printf("enter the elements of the array:");
	fflush(stdin);fflush(stdout);
	for(int i=0;i<len;i++)
	{
		scanf("%d",&arr[i]);
	}
	for(int i=0;i<len;i++)
		{
			printf("%d",arr[i]);
		}


}
