#include "stdio.h"

int reverse(int x)
{
	int res=0,y;
	while(x!=0)
	{
		y=x%10;
		res=res*10+y;
		x/=10;

	}
	return res;
}
int main()
{
	int n;
	printf("enter the number:");
	fflush(stdin);fflush(stdout);
	scanf("%d",&n);
	printf("the reverse is:%d",reverse(n));

}

