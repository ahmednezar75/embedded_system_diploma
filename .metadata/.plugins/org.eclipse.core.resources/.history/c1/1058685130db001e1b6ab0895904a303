#include "stdio.h"
int z;
int factorial(z)
{
	if (z==0)
	{
		return 1;
	}
	else
{
	return z*factorial(z-1);
	}
}
int main()
{
	char x[100];
	printf("enter a sentence:");
	fflush(stdin);fflush(stdout);
	gets(x);
printf("%s",x);
}

