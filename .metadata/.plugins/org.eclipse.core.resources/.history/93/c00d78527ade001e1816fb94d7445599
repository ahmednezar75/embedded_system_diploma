#include "stdio.h"
int find_Unique_number(int arr[], int len) {
    int unique = 0;
    for (int i = 0; i < len; i++) {
        unique ^= arr[i];
    }
    return unique;
}

int main() {
    int len;
    int arr[7]={4,2,5,2,5,7,4};

	len = sizeof(arr) / sizeof(arr[0]);
    printf("The unique number is: %d\n", find_Unique_number(arr, len));
    return 0;
}
