#include <stdio.h>
#include <stdlib.h>

int main()
{
	int n, arr[100], ele;
	printf("enter the number of elements:");
	fflush(stdout);
	scanf("%d",&n);
	for(int i=0;i<n;i++)
	{
		arr[i]=11*(i+1);
		printf("%d\t",arr[i]);
	}
	printf("\n");
	printf("enter the element to be searched: ");
	fflush(stdout);
	scanf("%d",&ele);
	for(int i=0;i<n;i++)
	{
		if(arr[i]==ele)
			printf("number found at location= %d",i+1);
	}
}
--------------------------------------------------------------------------
#include "Stdio.h"
void main()
{
	int x;
	int new_element;
	int location;
	printf("enter number of elements:\n");
	fflush(stdin);fflush(stdout);
	scanf("%d",&x);

	for (int i=0;i<x;i++)
	{
		int y[x];
		y[i]=i+1;
		printf("%d\t",y[i]);
	}
	printf("\n");
	printf("enter the element to be inserted:\n");
	fflush(stdin);fflush(stdout);
	scanf("%d",&new_element);
	printf("enter the location:\n");
	fflush(stdin);fflush(stdout);
	scanf("%d",&location);

}
-------------------------------------------------------------------------------
#include "Stdio.h"
int prime(int t,int x)
{
	int f;
	for(int i=2;i<x;i++)
	{
	 f=t+i-1;
	for(int j=2;j<x;j++)
	{
	 if (f%i==0)
	{
		printf("%d",f);
	}
	}
	}
	return f;
}
void main()
{
	int x,y;
	printf("enter first number:\n");
	fflush(stdout);
	scanf("%d",&x);
	printf("enter second number:\n");
	fflush(stdout);
	scanf("%d",&y);
	prime(x,y);

}
