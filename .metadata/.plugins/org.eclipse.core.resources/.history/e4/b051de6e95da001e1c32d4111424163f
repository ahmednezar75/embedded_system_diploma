#include "Stdio.h"
void prime(int t,int x)
{
	int i;
	int j;
	for(i=t;i<x;i++)
	{
		for(int j=2;j<x;j++)
		{
			if (i==j)
			{
				break;
			}
			else if  (i%j ==0)
				 {
				printf("%d\t",i);
				 }

		}
	}
}

void main()
{
	int x,y;
	printf("enter first number:\n");
	fflush(stdout);
	scanf("%d",&x);
	printf("enter second number:\n");
	fflush(stdout);
	scanf("%d",&y);
	printf("Prime numbers between %d and %d are:\n", x, y);
	prime(x, y);


}
