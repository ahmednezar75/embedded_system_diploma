#include "stdio.h"
int find_Unique_number(int arr[], int len) {
	int unique = 0;
	for (int i = 0; i < len; i++) {
		unique ^= arr[i];
	}
	return unique;
}

int main()
{
	char arr1[100];
	char arr2[100];
	printf("enter the name:");
	fflush(stdin);fflush(stdout);
	scanf("%s",&arr1);
	fflush(stdin);fflush(stdout);
	scanf("%s",&arr2);
	printf("the name is %s %s",arr1,arr2);
}
