#include "stdio.h"
int z,sum=1;
void factorial(z)
{
for(;z<1;z--)
{
	sum*=z;
	factorial(z);
	}
}
int main()
{
	int x;
	printf("enter a positive integer:");
	fflush(stdin);fflush(stdout);
	scanf("%d",&x);
factorial(x);
printf("%d",sum);
}

