#include "stdio.h"
int findUnique(int a[], int n) {
    int unique = 0;
    for (int i = 0; i < n; i++) {
        unique ^= a[i];
    }
    return unique;
}

int main() {
    int a[];
    int len;
	printf("enter the array length:");
	fflush(stdin);fflush(stdout);
	scanf("%d",&len);
	printf("enter the array elements:");
	for (int i=0;i<len;i++)
		scanf("%d",&a[i]);
	int n = sizeof(a) / sizeof(a[0]);
    printf("The unique number is: %d\n", findUnique(a, len));
    return 0;
}


