int findUnique(int a[], int n) {
    int unique = 0;
    for (int i = 0; i < n; i++) {
        unique ^= a[i];
    }
    return unique;
}

int main() {
    int a[] = {4, 2, 4};
    int n = sizeof(a) / sizeof(a[0]);
    printf("The unique number is: %d\n", findUnique(a, n));
    return 0;
}
