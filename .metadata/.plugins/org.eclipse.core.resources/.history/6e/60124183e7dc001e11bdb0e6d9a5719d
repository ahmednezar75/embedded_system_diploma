#include <stdio.h>
int i,j;

int prime( i, j)
{
	for(;i<=j;i++)
	{
	for (int z=2;z<j;z++)
	{
		if(i%z ==0 && i!=z)
		{
			return 0;
		}
		else
		{
			return 1;
		}
	}
	print(i);
	}
}
void print(i)
{
if (prime(i,j)==1)
{
	printf("%d",i);
	}
}

void main() {
	int x,y;
	printf("Enter the first number: \n");
	fflush(stdout);fflush(stdin);
	scanf("%d",&x);
	printf("Enter the second number: \n");
	fflush(stdout);fflush(stdin);
	scanf("%d",&y);
	prime(x,y);

}
